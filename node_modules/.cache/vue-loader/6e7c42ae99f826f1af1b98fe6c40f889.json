n                <div :style=\"{backgroundColor: color}\" class=\"wall\">\n                    <img :src=\"item.img\">\n                </div>\n                <div class=\"forniture\">\n                    <transition-group name=\"slideIn\">\n                        <div class=\"view1\" v-if=\"livingroom\">\n                            <img id=\"sofa\" src=\"../assets/sofa.png\">\n                            <img id=\"lamp\" src=\"../assets/lamp.png\">\n                        </div>\n                    </transition-group>\n                  </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\n\nexport default {\n    data(){\n        return{\n            color: '#243E36',\n            livingroom: false\n        }\n    },\n    computed: {\n        ...mapGetters([\n            'showPainting',\n            'forSale'\n        ]),\n        painting() {\n            return this.showPainting.map((item) => {\n                return this.forSale.find((forSaleItem) => {\n                    return item === forSaleItem.id;\n                })\n            })\n        },\n\n    }\n}\n</script>\n\n<style scoped>\n\n.showroom {\n    margin: 5%;\n    display: flex;\n    justify-content: space-between;\n    font-weight: 300;\n    font-size: 1em;\n}\n.setting {\n    width: 20%;\n    background-color: #89817d;\n}\n.display {\n    width: 80%;\n    text-align: center;\n    position: relative;\n}\n.wall {\n    padding: 20%; \n}\n.forniture {\n    background-color: #d8cfcb;\n    padding: 5%;\n}\n.forniture img {\n    position: absolute;\n}\n#sofa {\n    top: 65%;\n    left: 31%;\n    width: 40%;\n}\n#lamp {\n    width: 18%;\n    top: 40%;\n    left: 70%;\n}\nimg {\n    width: 15%;\n}\n.wallColor {\n    display: flex;\n}\n</style>"]}]}